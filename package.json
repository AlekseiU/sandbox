	"scripts": {
		"env": "node ./configs/environment/createEnv.js",
		"run": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config ./configs/webpack/webpack.config.babel.js --open --mode=development",
		"build": "webpack --config ./configs/webpack/webpack.config.babel.js --mode=production",
		"lint": "run-p lint:*",
		"lint:types": "tsc --noEmit",
		"lint:eslint": "yarn eslint:check .",
		"lint:styles": "yarn stylelint",
		"eslint:check": "eslint --cache --cache-location=\"./node_modules/.cache/.eslintcache.json\" --max-warnings=0 --ext=\".js,.jsx,.ts,.tsx\"",
		"eslint:fix": "yarn eslint:check --fix",
		"stylelint:check": "stylelint --syntax scss src/**/*.{css,scss}",
		"stylelint:fix": "yarn stylelint:check --fix",
		"prettier:check": "prettier --check src/**/*.{css,scss,js,ts,tsx}",
		"prettier:fix": "prettier --write src/**/*.{css,scss,js,ts,tsx}"
	},
